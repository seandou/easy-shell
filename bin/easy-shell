#!/usr/bin/env node

'use strict';

var os = require('os');
var es = require('../lib/');

var argv = process.argv.slice(2);

// parse component and data
var component = argv.shift();
var msg = argv.shift();
var data = argv.shift().trim().split(os.EOL);

// process
if (component == 'list') {
    es.list(msg, data);
}